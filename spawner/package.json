  "dependencies": {
    "@types/dockerode": "^2.5.34",
    "@types/express": "^4.17.8",
    "dockerode": "^3.2.1",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "npm": "^6.14.8",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.6.0"
  }
}
