{
  "overrides": [
    {
      "files": "*.test.js",
      "options": {
        "semi": true
      }
    },
    {
      "files": ["*.html", "*.js", "*.vue", "*.json"],
      "options": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "arrowParens": "avoid",
        "proseWrap": "preserve",
        "printWidth": 120,
        "vueIndentScriptAndStyle": true
      }
    }
  ]
}
