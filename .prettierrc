{
  "overrides": [
    {
      "files": "*.test.js",
      "options": {
        "semi": true
      }
    },
    {
      "files": ["*.html","*.vue", "*.json"],
      "options": {
        "trailingComma": "es5",
        "tabWidth": 2,
        "semi": true,
        "singleQuote": true,
        "arrowParens": "avoid",
        "proseWrap": "preserve",
        "printWidth": 120,
        "vueIndentScriptAndStyle": false
      }
    }, {
      "files": ["*.js"],
      "options": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "arrowParens": "avoid",
        "proseWrap": "preserve",
        "printWidth": 120,
        "vueIndentScriptAndStyle": false
      }
    }
    
  ]
}
